{
  "dashboard": {
    "id": null,
    "title": "Catalytic Computing - System Metrics",
    "description": "Comprehensive system metrics dashboard for Catalytic Computing platform",
    "tags": ["catalytic-computing", "system-metrics", "performance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Response Times & Throughput",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 1000}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"catalytic-api\"}[5m])) * 1000",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"catalytic-api\"}[5m])) * 1000",
            "legendFormat": "50th Percentile",
            "refId": "B"
          },
          {
            "expr": "avg(rate(http_request_duration_seconds_sum{job=\"catalytic-api\"}[5m]) / rate(http_request_duration_seconds_count{job=\"catalytic-api\"}[5m])) * 1000",
            "legendFormat": "Average",
            "refId": "C"
          }
        ]
      },
      {
        "id": 2,
        "title": "API Request Rate (req/sec)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"catalytic-api\"}[5m])) by (method)",
            "legendFormat": "{{method}} requests/sec",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "GPU Utilization & Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "nvidia_gpu_utilization_gpu",
            "legendFormat": "GPU {{instance}} Utilization",
            "refId": "A"
          },
          {
            "expr": "nvidia_gpu_memory_used_bytes / nvidia_gpu_memory_total_bytes * 100",
            "legendFormat": "GPU {{instance}} Memory",
            "refId": "B"
          },
          {
            "expr": "nvidia_gpu_temperature_celsius",
            "legendFormat": "GPU {{instance}} Temperature (Â°C)",
            "refId": "C"
          }
        ]
      },
      {
        "id": 4,
        "title": "Memory Efficiency (28,571x Target)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 10000},
                {"color": "green", "value": 25000}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        },
        "targets": [
          {
            "expr": "catalytic_memory_efficiency_ratio",
            "legendFormat": "Current Efficiency",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Processing Speed Improvement (649x Target)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 400},
                {"color": "green", "value": 600}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "catalytic_processing_speed_ratio",
            "legendFormat": "Current Speed Ratio",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Database Connection Pool & Query Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "db_connections_active{job=\"saas-api\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "db_connections_idle{job=\"saas-api\"}",
            "legendFormat": "Idle Connections",
            "refId": "B"
          },
          {
            "expr": "db_connections_waiting{job=\"saas-api\"}",
            "legendFormat": "Waiting Connections",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "95th Percentile Query Time (ms)",
            "refId": "D"
          }
        ]
      },
      {
        "id": 7,
        "title": "Memory Usage by Component",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\"catalytic-.*\"}",
            "legendFormat": "{{name}}",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes{job=~\"catalytic-.*\"}",
            "legendFormat": "{{job}} Process Memory",
            "refId": "B"
          }
        ]
      },
      {
        "id": 8,
        "title": "Docker Container Health",
        "type": "table",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 20},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            }
          }
        },
        "targets": [
          {
            "expr": "container_last_seen{name=~\"catalytic-.*\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"__name__": true, "instance": true, "job": true},
              "renameByName": {
                "name": "Container",
                "Value": "Last Seen"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Redis Cache Performance",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 20},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate",
            "refId": "A"
          },
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Memory Usage",
            "refId": "B"
          }
        ]
      },
      {
        "id": 10,
        "title": "System Error Rates",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 20},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (service)",
            "legendFormat": "5xx Errors - {{service}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m])) by (service)",
            "legendFormat": "4xx Errors - {{service}}",
            "refId": "B"
          }
        ]
      },
      {
        "id": 11,
        "title": "Lattice Operations Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(lattice_operation_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "avg(rate(lattice_operation_duration_seconds_sum[5m]) / rate(lattice_operation_duration_seconds_count[5m])) * 1000",
            "legendFormat": "Average Duration",
            "refId": "B"
          },
          {
            "expr": "rate(lattice_operations_total[5m])",
            "legendFormat": "Operations/sec",
            "refId": "C"
          }
        ]
      },
      {
        "id": 12,
        "title": "XOR Transform & Path Finding Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "avg(xor_transform_duration_ms)",
            "legendFormat": "XOR Transform Avg (ms)",
            "refId": "A"
          },
          {
            "expr": "avg(path_finding_duration_ms)",
            "legendFormat": "Path Finding Avg (ms)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(xor_transform_duration_ms_bucket[5m]))",
            "legendFormat": "XOR Transform 95th (ms)",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(path_finding_duration_ms_bucket[5m]))",
            "legendFormat": "Path Finding 95th (ms)",
            "refId": "D"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "query": "label_values(up, environment)",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{environment=\"$environment\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "expr": "changes(kube_deployment_status_replicas{namespace=\"catalytic-lattice\"}[5m]) > 0"
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "rgba(255, 96, 96, 1)",
          "expr": "ALERTS{alertstate=\"firing\"}"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}