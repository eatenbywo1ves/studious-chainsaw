# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Stripe Price IDs
STRIPE_FREE_PRICE_ID=price_free
STRIPE_STARTER_PRICE_ID=price_starter_monthly
STRIPE_PROFESSIONAL_PRICE_ID=price_professional_monthly
STRIPE_ENTERPRISE_PRICE_ID=price_enterprise_monthly

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/catalytic_saas
DATABASE_URL_POOLING=postgresql://username:password@localhost:5432/catalytic_saas?pgbouncer=true

# Redis Configuration
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# Email Configuration (SendGrid)
SENDGRID_API_KEY=SG.your-sendgrid-api-key-here
SENDGRID_FROM_EMAIL=noreply@catalyticcomputing.com
SENDGRID_FROM_NAME=Catalytic Computing

# Email Configuration (AWS SES)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_SES_FROM_EMAIL=noreply@catalyticcomputing.com

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_TELEMETRY=true
NEXT_PUBLIC_ENABLE_BETA_FEATURES=false

# Grafana Configuration
GRAFANA_URL=http://localhost:3030
GRAFANA_API_KEY=your-grafana-api-key-here

# Prometheus Configuration
PROMETHEUS_URL=http://localhost:9090

# Ghidra Configuration
GHIDRA_INSTALL_DIR=/path/to/ghidra
GHIDRA_PROJECTS_DIR=/path/to/ghidra/projects

# Security
ENCRYPTION_KEY=your-32-character-encryption-key-here
HASH_SALT=your-hash-salt-here

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# File Upload
MAX_FILE_SIZE=50MB
UPLOAD_DIR=./uploads

# Monitoring
SENTRY_DSN=https://your-sentry-dsn-here
NEW_RELIC_LICENSE_KEY=your-new-relic-license-key